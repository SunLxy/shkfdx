{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "importHelpers": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
  },
  "include": [
    "src"
  ]
}