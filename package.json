{
  "name": "root",
  "private": true,
  "scripts": {
    "build:ui": "npm run-script build --workspace shkfdx-ui",
    "watch": "npm run-script watch --workspace shkfdx-ui",
    "build:website": "npm run-script build --workspace website",
    "build:shkfdx3": "npm run-script build --workspace shkfdx3",
    "start": "npm run-script dev --workspace shkfdx3",
    "build": "npm run build:ui && npm run build:website && npm run build:shkfdx3"
  },
  "workspaces": [
    "packages/*",
    "website",
    "shkfdx3"
  ],
  "dependencies": {},
  "devDependencies": {
    "@carefrees/rslib": "~0.0.10",
    "lerna": "^8.1.2",
    "tsbb": "4.2.5",
    "typescript": "5.8.2"
  }
}